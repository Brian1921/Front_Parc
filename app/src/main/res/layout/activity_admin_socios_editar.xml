<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03052E"
    tools:context=".admin_socios_editar">

        <RelativeLayout
            android:id="@+id/group_2"
            android:layout_width="match_parent"
            android:layout_height="91dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="68dp"
            android:background="#D9D9D9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="EDITAR"
                android:textAlignment="textEnd"
                android:textColor="#03052E"
                android:textSize="20sp" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="74dp"
                    android:layout_height="match_parent"
                    android:layout_alignLeft="@id/textView6"
                    android:layout_marginLeft="139dp"
                    app:srcCompat="@mipmap/logo" />

        </RelativeLayout>

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="181dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="180dp"
            android:textColor="@color/white"
            android:text="Nombre"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/group_2"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/editNom_admin_update_socio"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="101dp"
            android:ems="10"
            android:background="@drawable/custom_input"
            android:hint="Editar Nombre"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#CCD1D1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="180dp"
            android:textColor="@color/white"
            android:text="Apellido"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editNom_admin_update_socio"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/editApe_admin_update_socio"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="101dp"
            android:ems="10"
            android:background="@drawable/custom_input"
            android:hint="Editar Apellido"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#CCD1D1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="178dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="178dp"
            android:textColor="@color/white"
            android:text="Telefono"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editApe_admin_update_socio"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/editTel_admin_update_socio"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="101dp"
            android:ems="10"
            android:hint="Editar Telefono"
            android:inputType="phone"
            android:maxLength="10"
            android:background="@drawable/custom_input"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#CCD1D1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="189dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="188dp"
            android:textColor="@color/white"
            android:text="Email"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTel_admin_update_socio"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/editEm_admin_update_socio"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="101dp"
            android:ems="10"
            android:background="@drawable/custom_input"
            android:hint="Editar Email"
            android:inputType="textEmailAddress"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#CCD1D1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView18"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btnEditar_admin_socio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="162dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="162dp"
            android:textColor="#03052E"
            android:text="Editar"
            android:backgroundTint="#D9D9D9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editEm_admin_update_socio"
            tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>