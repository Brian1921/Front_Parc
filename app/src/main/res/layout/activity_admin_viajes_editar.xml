<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03052E"
    tools:context=".admin_viajes_editar">
    <RelativeLayout
        android:id="@+id/group_2"
        android:layout_width="match_parent"
        android:layout_height="91dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="68dp"
        android:background="#D9D9D9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="EDITAR"
            android:textAlignment="textEnd"
            android:textColor="#03052E"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="74dp"
            android:layout_height="match_parent"
            android:layout_alignLeft="@id/textView6"
            android:layout_marginLeft="139dp"
            app:srcCompat="@mipmap/logo" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="181dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="180dp"
        android:textColor="@color/white"
        android:text="Fecha de Salida"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group_2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtFeSal_admin_update_viaje"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="101dp"
        android:editable="false"
        android:ems="10"
        android:background="@drawable/custom_input"
        android:textColor="@color/black"
        android:textColorHint="#CCD1D1"
        android:hint="Fecha de Salida"
        android:inputType="date"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="180dp"
        android:textColor="@color/white"
        android:text="Fecha de Llegada"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtFeSal_admin_update_viaje"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtFeLle_admin_update_viaje"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="101dp"
        android:editable="false"
        android:ems="10"
        android:background="@drawable/custom_input"
        android:textColor="@color/black"
        android:textColorHint="#CCD1D1"
        android:hint="Fecha de Llegada"
        android:inputType="date"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="178dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="178dp"
        android:textColor="@color/white"
        android:text="Destino"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtFeLle_admin_update_viaje"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtDestino_admin_update_viaje"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="101dp"
        android:background="@drawable/custom_input"
        android:ems="10"
        android:hint="Editar Destino"
        android:maxLength="10"
        android:inputType="date"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="#CCD1D1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="189dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="188dp"
        android:textColor="@color/white"
        android:text="ID Barco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtDestino_admin_update_viaje"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtIdBarco_admin_update_viaje"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:background="@drawable/custom_input"
        android:hint="Editar ID Barco"
        android:inputType="textEmailAddress"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="#CCD1D1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="189dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="188dp"
        android:textColor="@color/white"
        android:text="ID PatrÃ³n"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtIdBarco_admin_update_viaje"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtIdPatron_admin_update_viaje"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:background="@drawable/custom_input"
        android:hint="Editar ID Barco"
        android:inputType="textEmailAddress"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="#CCD1D1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        tools:ignore="MissingConstraints" />
    <Button
        android:id="@+id/btnEditar_admin_viaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="162dp"
        android:textColor="#03052E"
        android:text="Editar"
        android:backgroundTint="#D9D9D9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtIdPatron_admin_update_viaje"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>